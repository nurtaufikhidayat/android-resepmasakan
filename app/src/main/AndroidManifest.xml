<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.selfproject.hotelreservation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.nurtaufik.resepmasakan.MainActivity"></activity>
        <activity android:name="com.example.nurtaufik.resepmasakan.DetailActivity"></activity>
        <activity android:name="com.example.nurtaufik.resepmasakan.MenuActivity"></activity>
        <activity android:name="com.example.nurtaufik.resepmasakan.SplashActivity"
            android:theme="@style/SplashOnly">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>